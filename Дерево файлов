project                //корневая папка мода
├── mod.hjson          //характеристики мода
├── content            //папка с контентом  (код мода)
│   ├── items          //папка с ресурсами
│   ├── blocks         //папка с блоками
│   ├── mechs          //папка с мехами
│   ├── liquids        //папка с жидкостями
│   ├── units          //папка с юнитами
│   └── zones          //папка с игровыми зонами
├── maps               //папка с картами мода
├── bundles            //папка с переводами мода
├── sounds             //папка с пользовательскими звуками
├── schematics         //папка со схемами мода
├── scripts            //папка со скриптами
├── sprites-override   //папка спрайтов для
│                      //переопределения игрового контента
├── sprites            //папка со спрайтами игры
│   ├── items          //папка с ресурсами
│   ├── blocks         //папка с блоками
│   ├── mechs          //папка с мехами
│   ├── liquids        //папка с жидкостями
│   ├── units          //папка с юнитами
│   └── zones          //папка с зонами и компанией
└──
